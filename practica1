const int ledPIN= 3;
const int ledPIN1=5;
void setup() {
  Serial.begin(9600);  
  pinMode(ledPIN,OUTPUT);
  pinMode(ledPIN,OUTPUT);
  }

void loop() {
  digitalWrite (ledPIN,HIGH);
  delay(8000);
  digitalWrite (ledPIN,LOW);
  delay (700);
digitalWrite (ledPIN1,HIGH);
  delay(8000);
  digitalWrite (ledPIN1,LOW);
  delay (700);
}
